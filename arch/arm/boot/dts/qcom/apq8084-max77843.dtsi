&soc {
	i2c@13 {
		cell-index = <13>;
		compatible = "i2c-gpio";
		gpios = <&msmgpio 53 0 /* sda */
			&msmgpio 54 0 /* scl */
			>;
		i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;

		max77843@49 {
			compatible = "max77843,i2c";
			reg = <0x66>;
			interrupt-parent = <&msmgpio>;
			interrupts = <102 0>;
			max77843,irq-gpio = <&msmgpio 102 0x00>;
			max77843,wakeup = <1>;
			haptic,mode = <1>;
			haptic,divisor = <128>;
		};

		smb1357-charger@1c {
			compatible = "smb1357-charger,i2c";
			reg = <0x1c>;
		};
	};
};

